{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "everyworkflow/*"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mongodb": "*",
        "doctrine/annotations": "^1.13",
        "everyworkflow/auth-bundle": "dev-main",
        "everyworkflow/base-bundle": "dev-main",
        "everyworkflow/cms-bundle": "dev-main",
        "everyworkflow/crm-bundle": "dev-main",
        "everyworkflow/dam-bundle": "dev-main",
        "everyworkflow/ecommerce-bundle": "dev-main",
        "everyworkflow/panel-bundle": "dev-main",
        "everyworkflow/pim-bundle": "dev-main",
        "everyworkflow/swagger-bundle": "dev-main",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^2.0.1",
        "symfony/framework-bundle": "6.0.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "6.0.*",
        "symfony/property-info": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/validator": "6.0.*",
        "symfony/yaml": "6.0.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpmd/phpmd": "^2.11",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/browser-kit": "6.0.*",
        "symfony/css-selector": "6.0.*",
        "symfony/debug-bundle": "6.0.*",
        "symfony/phpunit-bridge": "6.0.*",
        "symfony/stopwatch": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/var-dumper": "6.0.*",
        "symfony/web-profiler-bundle": "6.0.*",
        "vimeo/psalm": "^4.15"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
