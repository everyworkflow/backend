{
    "name": "everyworkflow/pim-bundle",
    "description": "Pim bundle for every workflow project.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "EveryWorkflow",
            "email": "everyworkflow@gmail.com"
        }
    ],
    "require": {
        "everyworkflow/base-bundle": "*",
        "everyworkflow/dam-bundle": "*",
        "everyworkflow/cms-bundle": "*",
        "everyworkflow/catalog-category-bundle": "*",
        "everyworkflow/catalog-product-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "EveryWorkflow\\PimBundle\\": "./"
        },
        "exclude-from-classmap": [
            "Resources",
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryWorkflow\\PimBundle\\Tests\\": "Tests/"
        }
    }
}
