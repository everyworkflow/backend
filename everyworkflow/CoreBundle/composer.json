{
    "name": "everyworkflow/core-bundle",
    "description": "Core bundle for every workflow project.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "EveryWorkflow",
            "email": "everyworkflow@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/annotations": "^2.0",
        "doctrine/inflector": "^2.0",
        "symfony/cache": "6.3.*",
        "symfony/uid": "6.3.*",
        "nelmio/cors-bundle": "^2.3",
        "symfony/monolog-bundle": "^3.8",
        "symfony/validator": "6.3.*",
        "symfony/serializer-pack": "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "EveryWorkflow\\CoreBundle\\": "./"
        },
        "exclude-from-classmap": [
            "Resources",
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryWorkflow\\CoreBundle\\Tests\\": "Tests/"
        }
    }
}
